{
  "name": "oats",
  "version": "1.0.0",
  "description": "Open Asset Tracking System (OATS)",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/server.ts",
    "start": "node dist/server.js",
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thehouseplant/oats.git"
  },
  "keywords": [
    "asset",
    "asset-management",
    "asset-tracking",
    "inventory",
    "inventory-management",
    "inventory-tracking"
  ],
  "author": "Sean Collins <sean.collins@outlook.com>",
  "license": "MIT",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/thehouseplant/oats/issues"
  },
  "homepage": "https://github.com/thehouseplant/oats#readme",
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.60.1",
    "@opentelemetry/instrumentation-express": "^0.51.0",
    "@opentelemetry/instrumentation-http": "^0.202.0",
    "@opentelemetry/sdk-metrics": "^2.0.1",
    "@opentelemetry/sdk-node": "^0.202.0",
    "@opentelemetry/sdk-trace-node": "^2.0.1",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "pg": "^8.16.2",
    "pm2": "^6.0.8",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^5.1.3",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^30.0.0",
    "@types/k6": "^1.0.2",
    "@types/node": "^24.0.3",
    "@types/pg": "^8.15.4",
    "@types/winston": "^2.4.4",
    "eslint": "^9.29.0",
    "jest": "^30.0.2",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
